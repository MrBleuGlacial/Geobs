declare namespace gmd="http://www.isotc211.org/2005/gmd";
declare namespace gco="http://www.isotc211.org/2005/gco";
declare namespace fra="http://www.cnig.gouv.fr/2005/fra";
for $herit at $pos in //gmd:pointOfContact/gmd:CI_ResponsibleParty
	let $roleCode := $herit/gmd:role/gmd:CI_RoleCode/@codeListValue
where $roleCode = 'owner'
return concat('- ', string($herit/gmd:organisationName),' (', data($herit/gmd:role/gmd:CI_RoleCode/@codeListValue), ')')
