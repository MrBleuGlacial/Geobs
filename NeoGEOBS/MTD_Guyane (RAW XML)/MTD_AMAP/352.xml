        <gmd:CI_Citation>
          <gmd:title>
            <gco:CharacterString>Inventaires Papetiers to assess the forest resources in French Guiana between 1972 and 1976.</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>1972-01-01</gco:Date>
              </gmd:date>
              <gmd:dateType>
