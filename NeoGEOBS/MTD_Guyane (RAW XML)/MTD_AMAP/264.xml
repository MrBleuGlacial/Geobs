        <gmd:CI_Citation>
          <gmd:title>
            <gco:CharacterString>Guyaflux: a micrometeorological tower for studying greenhouse gases exchanges between the guianese amazonian forest and atmosphere since 2003</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>2003-01-01</gco:Date>
              </gmd:date>
              <gmd:dateType>
