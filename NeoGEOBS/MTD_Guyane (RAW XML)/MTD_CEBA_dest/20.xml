        <gmd:CI_Citation>
          <gmd:title>
            <gco:CharacterString>Detail of a dataset for French Guiana that estimates the aboveground live woody vegetation carbon density from multi-sensor satellite data in 2012.</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>2012-01-29</gco:Date>
              </gmd:date>
              <gmd:dateType>
