          <gmd:title>
            <gco:CharacterString>Mesures des flux de CO2 atmosphérique dans le Bassin d'Arcachon</gco:CharacterString>
          </gmd:title>
          <gmd:alternateTitle>
            <gco:CharacterString>Eddy covariance</gco:CharacterString>
          </gmd:alternateTitle>
          <gmd:date>
